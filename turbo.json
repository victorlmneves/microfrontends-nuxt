{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "dev": {
            "cache": false,
            "persistent": true
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".nuxt/**", ".output/**", "dist/**"]
        },
        "lint": {
            "outputs": []
        },
        "lint:fix": {
            "outputs": [],
            "cache": false
        },
        "format": {
            "outputs": [],
            "cache": false
        },
        "type-check": {
            "outputs": []
        },
        "clean": {
            "outputs": []
        }
    }
}
