{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "Node",
        "allowJs": true,
        "checkJs": false,
        "jsx": "preserve",
        "types": ["node"],
        "baseUrl": ".",
        "paths": {},
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": ["components/**/*.vue", "**/*.ts"]
}
